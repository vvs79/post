<h1> МОЯ ПЕРША ТАБЛИЦЯ </h1>

<table>
  <thead>
    <tr>
      <th>ІМЯ</th>
      <th>ВІК</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groupes.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', '#' %></td>  <!-- edit_user_path(user) -->
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Ви впевнені?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

